<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.form {
			empty-cells: show;
		}
	</ui:style>
	<g:DialogBox autoHide='false' modal='true'>
		<g:caption>EDIT WSDL</g:caption>
		<g:HTMLPanel>
			<table class='{style.form}' width='100%'>
				<tr>
					<td>Name:</td>
					<td>
						<g:TextBox ui:field='name' visibleLength='60' />
					</td>
				</tr>
				<tr>
					<td>Description:</td>
					<td>
						<g:TextArea ui:field='description' characterWidth='60'
							visibleLines='4' />
					</td>
				</tr>
				<tr>
					<td>WSDL:</td>
					<td>
						<g:TextBox ui:field='wsdl' visibleLength='60' />
					</td>
				</tr>
			</table>
			<table width='100%'>
				<tr>
					<td align='right'>
						<g:Button ui:field='cancelBtn'>Cancel</g:Button>
						<g:SubmitButton ui:field='okBtn'>OK</g:SubmitButton>
					</td>
				</tr>
			</table>
		</g:HTMLPanel>
	</g:DialogBox>
</ui:UiBinder> 
