	<g:HTMLPanel>
		<div
			style="border-bottom-style:solid; border-bottom-width:1px; min-height:24px; text-align:left; margin-top: 20px">
			<span style="vertical-align:middle;">List of previously saved wsdl references:</span>
		</div>
		<div style="margin-top: 10px;">
			<div style="width: 100%; text-align:right;">
				<g:TextBox ui:field='searchBox' visibleLength='30' enabled='false'/>
			</div>
		</div>
		<div style="position:absolute;left:0px;top:90px;right:15px;heigth:24px;"
			class='header'>
			<table style="width:100%">
				<thead>
					<tr>
						<th style="width:20%">NAME</th>
						<th style="width:60%">DESCRIPTION</th>
						<th style="width:20%">SAVING DATE</th>
					</tr>
				</thead>
			</table>
		</div>
		<div
			style="position:absolute;left:0px;top:114px;right:0px;bottom:30px;overflow:hidden;"
			class='content'>
			<div
				style="position:absolute;top:0px;left:0px;right:0px;bottom:0px;overflow:scroll;">
				<table style="width:100%">
					<tbody ui:field='content' />
				</table>
			</div>
		</div>
		<div style="position:absolute;left:0px;right:0px;bottom:0px;height:30px;">
			<table style="width:100%">
				<tr>
					<td style="text-align:left;">
						<g:Button ui:field='deleteBtn'>Delete</g:Button>
					</td>
					<td style="text-align:right;">
						<g:Button ui:field='editBtn'>Edit...</g:Button>
                        <g:Button ui:field='duplicateBtn'>Duplicate</g:Button>
						<g:Button ui:field='newBtn'>New...</g:Button>
						<g:Button ui:field='openBtn'>Open</g:Button>
					</td>
				</tr>
			</table>
		</div>
	</g:HTMLPanel>
</ui:UiBinder> 