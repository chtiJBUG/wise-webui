<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.important {
			font-weight: bold;
		}
	</ui:style>
	<g:HTMLPanel>
		<div style="position:absolute;top:0px;left:0px;right:15px;heigth:28px;">
			<table border="1" style="width:100%">
				<thead>
					<tr>
						<th style="width:20%">NAME</th>
						<th style="width:60%">DESCRIPTION</th>
						<th style="width:20%">SAVING DATE</th>
					</tr>
				</thead>
			</table>
		</div>
		<div
			style="position:absolute;top:28px;left:0px;right:0px;bottom:0px;overflow:auto;">
			<table border="1" style="width:100%">
				<tbody ui:field='content'>
				</tbody>
			</table>
		</div>
	</g:HTMLPanel>
</ui:UiBinder> 