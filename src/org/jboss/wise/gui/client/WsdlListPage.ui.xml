<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:wise="urn:import:org.jboss.wise.gui.client">
	<ui:style>
		.important {
			font-weight: bold;
		}
	</ui:style>
	<g:HTMLPanel>
		<div id="toolbar"
			style="position: absolute; top:0px; left:0px; right:0px; height: 24px; border-bottom-style:solid; border-bottom-width:1px; text-align:right;">
			<span style="margin-right: 10px;">Welcome mail@domain.com</span>
			<img src="edit.png" style="vertical-align:middle; cursor:pointer;" />
			<img src="logout.png" style="vertical-align:middle; cursor:pointer;" />
		</div>
		<div id="content"
			style="position: absolute; top:26px; left:0px; right:0px; bottom: 0px;">
			<div
				style="border-bottom-style:solid; border-bottom-width:1px; min-height:24px; text-align:left; margin-top: 20px">
				<span style="vertical-align:middle;">List of previously saved wsdl references:</span>
			</div>
			<div style="margin-top: 10px;">
				<div style="width: 100%; text-align:right;">
					<g:TextBox ui:field='searchBox' visibleLength='30' />
				</div>
			</div>
			<div style="position:absolute;left:0px;top:80px;right:0px;bottom:30px">
				<wise:WsdlList ui:field="list" />
			</div>
			<div style="position:absolute;left:0px;right:0px;bottom:0px;height:30px;">
				<table style="width:100%">
					<tr>
						<td style="text-align:left;">
							<g:Button ui:field='deleteBtn'>Delete</g:Button>
						</td>
						<td style="text-align:right;">
							<g:Button ui:field='editBtn'>Edit...</g:Button>
							<g:Button ui:field='newBtn'>New...</g:Button>
							<g:Button ui:field='openBtn'>Open</g:Button>
						</td>
					</tr>
				</table>
			</div>
		</div>
	</g:HTMLPanel>
</ui:UiBinder> 