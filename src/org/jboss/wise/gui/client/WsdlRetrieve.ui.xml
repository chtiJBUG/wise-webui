	<g:DialogBox autoHide='false' modal='true' animationEnabled='true'>
		<g:caption>WSDL RETRIEVE</g:caption>
		<g:HTMLPanel>
			<table width='100%'>
				<tr>
					<td>Retrieve wsdl from:</td>
					<td>
						<span ui:field='wsdlUrl' />
					</td>
				</tr>
				<tr>
					<td>User Name:</td>
					<td>
						<g:TextBox ui:field='userName' visibleLength='30' />
					</td>
				</tr>
                <tr>
                    <td>Password:</td>
                    <td>
                        <g:PasswordTextBox ui:field='password'
                            visibleLength='15' maxLength='15' />
                    </td>
                </tr>
			</table>
			<table width='100%'>
				<tr>
					<td align='right'>
						<g:Button ui:field='cancelBtn'>Cancel</g:Button>
						<g:SubmitButton ui:field='okBtn'>OK</g:SubmitButton>
					</td>
				</tr>
			</table>
		</g:HTMLPanel>
	</g:DialogBox>
</ui:UiBinder> 